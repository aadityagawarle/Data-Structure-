#include <stdio.h>
int main(){
	int a[100], n, i, key, flag = 0;
	
	scanf("%d", &n);
	
	for(i = 0; i < n; i++){
		scanf("%d", &a[i]);
	}
	
	scanf("%d", &key);
	for(i = 0; i < n; i++){
		if (a[i] == key){
			printf("found at position %d\n", i);
			flag = 1;
			break;
		}
	}
	if(flag==0){
		printf("%d not found\n", key);
	}
	return 0;
}
